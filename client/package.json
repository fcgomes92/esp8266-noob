{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@activejs/core": "^0.2.3",
    "@reach/router": "^1.3.4",
    "axios": "^0.19.2",
    "module-alias": "^2.2.2",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.2",
    "sass": "^1.26.11",
    "tailwindcss": "^1.9.4"
  },
  "scripts": {
    "start": "parcel ./src/index.html",
    "watch": "parcel watch ./src/index.html",
    "build": "parcel build --experimental-scope-hoisting ./src/index.html"
  },
  "_moduleAliases": {
    "~root": ".",
    "~": "./src",
    "~styles": "./src/styles",
    "~components": "./src/components",
    "~config": "./src/config",
    "~api": "./src/api",
    "~utils": "./src/utils"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
